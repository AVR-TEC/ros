<launch>
    <arg name="bag_DIR" default="/ext/Data/Round_2/release/car/training" />
    <arg name="tracklets_DIR" default="/ext2/round2_data/tracklets" />
    <arg name="dir" default="nissan_following_long" />
    <arg name="bag" default="nissan07.bag" />
    <arg name="rate" default="2." />
    <param name="use_sim_time" value="true" />
    <node name="rosbag" pkg="rosbag" type="play" args="-r $(arg rate) --clock $(arg bag_DIR)/$(arg dir)/$(arg bag)"/>
	<node name="velodyne" pkg="velodyne_pointcloud" type="cloud_node" args="">
        <param name="calibration" value="$(find velodyne_pointcloud)/params/32db.yaml" />
    </node>
	<node name="projection" pkg="projection" type="interact_tracklet.py" args="$(arg bag_DIR)/$(arg dir)/$(arg bag) $(arg tracklets_DIR)/$(arg dir)/tracklet_labels.xml $(find projection)/scripts/ost_new.yaml"/>
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find projection)/launch/config.rviz"  />
</launch>